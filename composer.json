{
  "name": "strux-php/framework",
  "type": "project",
  "description": "The StruxPHP framework.",
  "keywords": [
    "strux-php",
    "framework"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Kwetsi Mashiyana",
      "email": "strapcart.commerce@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Strux\\": "Strux/"
    },
    "classmap": [
      "src/"
    ],
    "files": [
      "Strux/Support/Helpers/functions.php",
      "Strux/Support/Helpers/custom.php"
    ]
  },
  "require": {
    "php": ">=8.2",
    "phpmailer/phpmailer": "^6.9",
    "vlucas/phpdotenv": "^5.6",
    "ext-pdo": "*",
    "ext-apcu": "*",
    "twig/twig": "^3.8",
    "league/plates": "^3.6",
    "monolog/monolog": "^3.9",
    "psr/container": "^2.0",
    "psr/simple-cache": "^3.0",
    "tuupola/cors-middleware": "^1.5",
    "psr/http-message": "^1.0 || ^2.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "nyholm/psr7": "^1.8",
    "nyholm/psr7-server": "^1.1",
    "filp/whoops": "^2.18",
    "psr/event-dispatcher": "^1.0",
    "firebase/php-jwt": "^6.10",
    "ext-fileinfo": "*",
    "ext-mbstring": "*"
  },
  "provide": {
    "psr/container-implementation": "1.1|2.0",
    "psr/http-server-handler-implementation": "^1.0",
    "psr/http-server-middleware-implementation": "^1.0",
    "psr/simple-cache-implementation": "1.0|2.0|3.0",
    "psr/event-dispatcher-implementation": "1.0"
  },
  "scripts": {
    "post-install-cmd": [
      "Strux\\Component\\Console\\CLI::createEnvFile"
    ]
  },
  "suggest": {
    "ext-apcu": "Required to use the APC cache driver."
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
